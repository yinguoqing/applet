#all{
    display: block;
    min-height: 100%;
    min-width: 100%;
    overflow: hidden;
    background-color: #f5f5f5;
}
#top{
    width: 100%;
    background-color: white;
    padding-top: 15rpx;
}
.picker{
    padding: 19rpx 26rpx;
    background-color: #FFFFFF;
}
.userinfo-avatar {
    position: absolute;
    top: 5%;
    right: 65rpx;
    width: 80rpx;
    height: 80rpx;
    border-radius: 50%;
}
.back2{
    position: absolute;
    top: 5%;
    right: 0;
}
.back{
    width: 40rpx;
}
.content{
    position: relative;
    width: 92%;
    height: 90rpx;
    line-height:90rpx;
    margin: 0 auto;
    border-bottom: 1px #e5e5e5 solid;
}
.text{
    position: absolute;
    height: 90rpx;
}
.telText{
    width:80%;
    height: 90rpx;
    margin-left:29%;
}
input{
    width:80%;
    height: 90rpx;
    margin-left:29%;
}
#address{
    border-bottom: 0;
}
#childInfo{
    border-left: 10rpx #6ad266 solid;
    padding-left: 20rpx;
    font-weight: bold;
    font-size: 30rpx;
}
.title{
    position: absolute;
}
.holder{
    position: absolute;
    left: 29%;
    color: #8d8d8d;
    width: 65%;
    height: 90rpx;
    overflow: auto;
}
.holder2{
    position: absolute;
    left: 29%;
    width: 65%;
    height: 90rpx;
    overflow: auto;
}
.del{
    color: #6ad266;
    background-color: #ffffff;
    height: 90rpx;
    line-height:90rpx;
}
#finish{
    background-color: #6ad266;
    color: white;
    width: 92%;
    height: 90rpx;
    line-height:90rpx;
    margin-top: 30rpx;
}
.button-hover {
    opacity: 0.5;
}
#birth{
    border-bottom: 0;
}
.num{
    color: #6ad266;
}
.add{
    color: #6ad266;
    position: absolute;
    top: 0;
    right: 0;
}
.addImg{
    width:40rpx;
    position: absolute;
    top: 26rpx;
    right: 75rpx;
}
.child_list{
    background-color: white;
}
.disable{
    background-color: #8d8d8d!important;
}