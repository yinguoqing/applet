#all{
    min-height: 100%;
    min-width: 100%;
    background-color: #f5f5f5;
}
.button{
    background-color:#6ad266;
    color:white;
    width:100%;
    height:90rpx;
    line-height:90rpx;
    margin-top:30rpx;
}
.button-hover{
    opacity: 0.5;
}
#first,#second,#third,#fourth,#fifth,#sixth{
    padding: 20rpx 3.5%;
    background-color: white;
    box-sizing: border-box;
}
#second,#third,#fourth,#fifth,#sixth{
    margin-top: 20rpx;
}
.title{
    border-bottom: 1px solid #e5e5e5;
    padding-bottom: 20rpx;
    margin-bottom: 20rpx!important;
    font-weight: bold;
}
.txt1{
    font-weight: bold;
    font-size: 30rpx;
}
.txt2{
    color: #8d8d8d;
    margin-top: 10rpx;
}
.title_text{
    border-left: 10rpx solid #6ad266;
    padding-left: 20rpx;
}
.grey{
    color: #8d8d8d;
}
.inline{
    display: inline-block;
    vertical-align: middle;
    height: 80rpx;
    line-height: 80rpx;
}
.input{
    width: 80%;
    height: 90rpx;
}
.inputPre{
    width: 20%;
}
#titleInput{
    margin-bottom: 0;
}
.title3{
    border-top: 1px solid #e5e5e5;
}