#all{
    min-height: 100%;
    min-width: 100%;
    overflow: hidden;
    background-color: #f5f5f5;
    font-size: 26rpx;
}
page{
    width: 100%;
    height: 100%;
}
.name{
    color: white;
    margin-top: 20rpx;
}
.logo{
    width: 180rpx;
    height: 180rpx;
    border-radius: 50%;
    margin-top: 100rpx;
}
#first{
    background-color: #6ad266;
    text-align: center;
    padding-bottom: 20rpx;
}
#second{
    background-color: white;
    line-height: 80rpx;
    height: 80rpx;
    padding: 0 3.5%;
    border-bottom: 1px solid #e5e5e5;
}
#third{
    line-height: 80rpx;
    height: 80rpx;
    padding: 0 3.5%;
    background-color: white;
}
#fourth{
    padding: 0 3.5%;
    background-color: white;
    margin-top: 20rpx;
}
.left{
    display: inline-block;
    width: 50%;
}
.right{
    display: inline-block;
    text-align: right;
    width: 50%;
}
.title{
    height: 80rpx;
}
.title_text {
    font-weight: bold;
    font-size: 30rpx;
}
.titleText{
    border-left: 10rpx #6ad266 solid;
    padding-left: 20rpx;
    margin-left: 4%;
    font-weight: bold;
    font-size: 30rpx;
}
.collect{
    width: 100%;
    margin-top: 20rpx;
    background-color: white;
    box-sizing: border-box;
}
.left2{
    display: inline-block;
    width: 30%;
    vertical-align: top;
    position: relative;
}
.right2{
    display: inline-block;
    width: 70%;
    height: 210rpx;
    padding-left: 5%;
    box-sizing: border-box;
    vertical-align: top;
    position: relative;
}
.img{
    width: 100%;
    height: 210rpx;
}
.img2 {
    width: 75%;
    position: absolute;
    top: 0;
    left: 0;
}
.biaoqian2{
    color: white;
    position: absolute;
    font-weight: bold;
    transform: rotate(-45deg);
    top: 39rpx;
    left: 15rpx;
    text-align: center;
}
.pingtuan{
    margin-top: 20rpx;
    padding-left: 5rpx;
    padding-right: 5rpx;
    color: #6ad266;
    border: 1px solid #6ad266;
    border-radius: 3px;
    display: inline-block;
}
.middle{
    position: absolute;
    bottom: 0;
    width: 95%;
}
.jiage{
    display: inline-block;
    width: 50%;
    text-align: right;
}
.baoming{
    display: inline-block;
    width: 45%;
    color: #8d8d8d;
}
.yuanjia{
    color: #8d8d8d;
    margin-bottom: 8rpx;
}
.tiefen1{
    color: white;
    display: inline-block;
    width: 100rpx;
    height: 30rpx;
    line-height: 30rpx;
    background: url("https://ffx.weiweitouzi.cn/data/upload/details/2x/biaoqian.png") no-repeat;
    background-size: 100%;
    text-align:center;
}
.tiefen2{
    color: #fe782f;
    font-weight: bold;
}
.fuhao{
    color: #fe782f;
}