#all{
    display: block;
    position: relative;
    overflow: auto;
}
#newAdd{
    bottom: 0;
    width: 100%;
    height: 100rpx;
    line-height: 100rpx;
    color: white;
    background-color: #6ad266;
    position: fixed;
}
#xinzeng{
    width: 60rpx;
    height: 60rpx;
    position: fixed;
    bottom: 20rpx;
    left: 185rpx;
    z-index: 2;
}
button{
    border-radius: 0;
}
.button-hover {
    opacity: 0.5;
}
.add{
    color: #8d8d8d;
    font-size: 30rpx;
    margin-top: 10rpx;
    border-bottom: 1px #e5e5e5 solid;
    padding-bottom: 30rpx;
}
.address{
    width: 92%;
    background-color: white;
    padding: 30rpx 4%;
}
.address+.address{
    margin: 30rpx auto 0 auto;
}
.address:last-child{
    margin-bottom: 100rpx;
}
.edit{
    width: 35rpx;
    height: 35rpx;
    vertical-align: middle;
}
.editText{
    vertical-align: middle;
    margin-left: 20rpx;
}
.delText{
    vertical-align: middle;
    margin-left: 20rpx;
}
.del{
    width: 35rpx;
    height: 35rpx;
    vertical-align: middle;
}
.edit_view{
    display: inline-block;
}
.del_view{
    display: inline-block;
    margin-left: 20rpx;
}
.radio{
    vertical-align: middle;
}
.radioText{
    vertical-align: middle;
}
.noData{
    text-align: center;
    background-color: white;
}
.noData_img{
    width: 150rpx;
    margin-top: 100rpx;
}
.left{
    display: inline-block;
    width: 40%;
}
.right{
    display: inline-block;
    width: 40%;
    text-align: right;
    margin-left: 20%;
}
.default{
    margin-top: 30rpx;
}